{
  "name": "@cartridge/arcade",
  "version": "0.0.0",
  "description": "Cartridge Arcade SDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cartridge-gg/arcade.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "browser": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./marketplace": {
      "types": "./dist/marketplace/index.d.ts",
      "import": "./dist/marketplace/index.mjs",
      "require": "./dist/marketplace/index.js"
    },
    "./marketplace/react": {
      "types": "./dist/marketplace/react.d.ts",
      "import": "./dist/marketplace/react.mjs",
      "require": "./dist/marketplace/react.js"
    }
  },
  "scripts": {
    "build:deps": "tsup && pnpm run build:esm && pnpm run build:iife && pnpm run build:dts",
    "build": "pnpm build:deps",
    "build:esm": "tsup --clean false --format esm --platform node",
    "build:iife": "tsup --clean false --format iife --platform browser --external node:*,tough-cookie,*.wasm",
    "build:dts": "tsup --clean false --dts-only",
    "pretest": "npm run format:check && npm run ts:check",
    "test": "vitest run",
    "test:ts": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "docs": "cd www && npm run start",
    "docs:build": "cd www && GIT_REVISION_OVERRIDE=${npm_config_git_revision_override} npm run build",
    "docs:build:version": "v=$(npm run info:version -s) && npm run docs:build --git-revision-override=${npm_config_git_revision_override=v$v}",
    "docs:version": "v=$(npm run info:version -s) && cd www && npm run version ${npm_config_version_override=$v}",
    "info:version": "npm pkg get version | xargs",
    "format": "biome format --fix .",
    "format:check": "biome format .",
    "format:ts": "pnpm format",
    "format:check:ts": "pnpm format:check",
    "ts:check": "tsc --noEmit --resolveJsonModule --project tsconfig.eslint.json",
    "ts:coverage": "type-coverage --at-least 95",
    "ts:coverage:report": "typescript-coverage-report",
    "lint": "biome lint --fix .",
    "lint:check": "biome lint .",
    "lint:ts": "pnpm lint",
    "lint:check:ts": "pnpm lint:check"
  },
  "keywords": [],
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "dependencies": {
    "@dojoengine/core": "catalog:",
    "@dojoengine/grpc": "catalog:",
    "@dojoengine/sdk": "catalog:",
    "@dojoengine/torii-client": "catalog:",
    "@dojoengine/torii-wasm": "catalog:",
    "@starknet-io/types-js": "^0.9.1",
    "eventemitter3": "^5.0.1",
    "starknet": "catalog:"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0-0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.11.10",
    "@types/react": "^19.0.3",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "tsup": "catalog:",
    "typedoc": "^0.27.0",
    "typedoc-plugin-markdown": "^4.4.1",
    "typescript": "catalog:",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^3.2.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
